<%if(success && success.length){%>
<div class=" text-center mt-2 show col-7 offset-3 alert alert-success alert-dismissible fase show" role="alert">
  <h4><%= success %></h4>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>

</div>

<% } %>

<%if(error && error.length){%>
    <div class=" text-center mt-2 show col-7 offset-3 alert alert-success alert-dismissible fase show" role="alert">
      <h4><%= error %></h4>
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    
    </div>
    
    <% } %>